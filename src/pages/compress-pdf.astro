---
import BaseLayout from "../partials/BaseLayout.astro";
import {
  downloadFile,
  edit_page,
  errors,
  tool,
  tools,
} from "../content/content";
import { Provider as ReduxProvider } from "react-redux";
import { configureStore } from "@reduxjs/toolkit";
import toolReducer from "../store";
import { ToolWrapper } from "../../components/ToolWrapper";
import { howToSchema } from "../how-to/how-to";
export const store = configureStore({
  reducer: {
    tool: toolReducer,
  },
});
---

<BaseLayout seoConfig={tool.Compress_PDF}>
  <ReduxProvider store={store}>
    <ToolWrapper
      client:load
      tools={tools}
      data={tool.Compress_PDF}
      lang=""
      errors={errors}
      edit_page={edit_page}
      pages={edit_page.pages}
      page={edit_page.page}
      downloadFile={downloadFile}
      features={tool.Compress_PDF.features}
      howTo={howToSchema}
      to={tool.Compress_PDF.to}
      seoTitle={tool.Compress_PDF.seoTitle}
    />
  </ReduxProvider>
</BaseLayout>
